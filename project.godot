; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Star Shield"
run/main_scene="res://Scenes/MainMenu.tscn"
run/low_processor_mode_sleep_usec=1000
config/icon="res://icon.png"

[autoload]

SceneVariables="*res://Scripts/SceneVariables.gd"
ScoreTracker="*res://Scripts/ScoreTracker.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false

[physics]

2d/thread_model=2
2d/cell_size=32
2d/large_object_surface_threshold_in_cells=256
common/enable_object_picking=false

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
threads/thread_model=2
quality/shadow_atlas/size.mobile=1024
quality/shadows/filter_mode=0
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/subsurface_scattering/quality=0
quality/subsurface_scattering/weight_samples=false
quality/voxel_cone_tracing/high_quality=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
